<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Food Rescue</name>
    <description>
        This application is to support the work that the food rescue organisation does in redistributing food in Townsville
    </description>
    <author email="dean.ditton@my.jcu.edu.au" href="http://cordova.io">
        Dean Ditton
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon src="www/res/drawable-ldpi/fr_logo.png" density="ldpi" />
        <icon src="www/res/drawable-mdpi/fr_logo.png" density="mdpi" />
        <icon src="www/res/drawable-hdpi/fr_logo.png" density="hdpi" />
        <icon src="www/res/drawable-xhdpi/fr_logo.png" density="xhdpi" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
